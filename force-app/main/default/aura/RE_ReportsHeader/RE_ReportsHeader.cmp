<aura:component implements="forceCommunity:availableForAllPageTypes" access="global">
    
    <div class="headingContainer"> 
        <div class="backButton slds-m-bottom_x-small">
            <lightning:icon iconName="utility:back" alternativeText="Back to Report"/>
            <a href="javascript:void(0)" onclick="{!c.backToReportPicklist}">{!$Label.c.back_to_report}</a>
        </div>      
        <div class="headingButton slds-grid slds-wrap">
            <h1 class="headingL1">{!$Label.c.report_details}</h1>
        </div>
    </div>
</aura:component>