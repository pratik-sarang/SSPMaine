<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Assignee__c</fullName>
    <description>Formula field containing Assignee Full Name derived from Owner Lookup on Claim Object.</description>
    <externalId>false</externalId>
    <formula>IF( ISBLANK(Owner:User.FirstName) ,  Owner:Queue.QueueName , Owner:User.FirstName+&apos; &apos;+Owner:User.LastName)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Assignee</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
